{
    "websiteName": "Nick's Blog",
    "postDirFormat": "YYYY-DD-MM",
    "postDateFormat": {
        "locale": "en-US",
        "dateOptions": {
            "year": "numeric",
            "month": "long",
            "day": "numeric"
        },
        "timeOptions": {
            "hour12": true,
            "hour": "2-digit",
            "minute": "2-digit"
        }
    },
    "postDir": "./posts",
    "postTemplateLoc": "./templates/post.html",
    "postOutputPath": "/posts",
    "publicDir": "./public",
    "indexTemplateLoc": "./templates/index.html"
}
